isbn = {1366-638X},
issn = {15729753},
journal = {Journal of Insect Conservation},
keywords = {Biodiversity,Diptera,Hymenoptera,Lepidoptera,Pollinator declines,Road ecology},
number = {5},
pages = {1029--1035},
title = {{Road mortality potentially responsible for billions of pollinating insect deaths annually}},
volume = {19},
year = {2015}
}
