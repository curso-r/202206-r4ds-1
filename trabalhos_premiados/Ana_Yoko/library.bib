@article{Voss2009,
abstract = {This report summarizes a decade of morphological and molecular research$\backslash$non the phylogenetic relationships of didelphid marsupials (Opossums), a$\backslash$nsubstantially intact radiation of New World metatherian mammals. We$\backslash$nreview the comparative morphology of Recent opossums, emphasizing those$\backslash$nanatomical systems from which taxonomically useful information is$\backslash$navailable for the., majority of living genera and species, namely the$\backslash$nintegument, cranium, and dentition. Morphological similarities and$\backslash$ndifferences among didelphids and other plesiomorphic marsupials$\backslash$n(caenolestids, microbiotheriids, dasyurids, and peramelids) are also$\backslash$ndescribed. These observations, representing evolved differences in$\backslash$ndiverse functional morphologic systems, together with karyotypic$\backslash$ninformation gleaned from the literature, provide the basis For coding$\backslash$n129 phylogenetic characters that we scored for 44 ingroup and seven$\backslash$noutgroup taxa.$\backslash$nPublished information about the size, internal organization. chromosomal$\backslash$nlocation. and physiological properties of Five nuclear genes (BRCAI,$\backslash$nDMPI, IRBP, RAGI, vWF) sequenced for this Study suggest that these loci$\backslash$nare unlinked, exist as single copies, are active in different tissues,$\backslash$nand encode protein products With widely divergent functions. All of the$\backslash$nsequenced fragments are long ({\textgreater}900 bp), free of ingroup alignment$\backslash$nambiguities, and translate to open reading frame. Nucleotide data from a$\backslash$ntotal of 7320 aligned sites were obtained from 43 ingroup) and seven$\backslash$noutgroup taxa.$\backslash$nSeparate parsimony, likelihood, and Bayesian analyses of these six data$\backslash$npartitions (morphology + karyotypes, five genes) resulted ill highly$\backslash$nCongruent estimates of didelphid phylogeny with few examples of conflict$\backslash$namong strongly Supported nodes. Analyses of concatenated sequences and$\backslash$ncombined (nonmolecular + sequence) datasets effectively summarize all of$\backslash$nthe common signal recovered from separate analyses: a completely$\backslash$nresolved ingroup phylogeny With high Support statistics at most nodes.$\backslash$nRemaining problems (not conclusively resolved in this study) include the$\backslash$nposition of the ingroup root and the relationships of three genera$\backslash$n(Chacodelphys, Cryptonanus, Tlacuatzin) within their respective$\backslash$nsuprageneric clades.$\backslash$nThe history of didelphid classification is reviewed, and all previous$\backslash$nsystems are found to contain nonmonophyletic groups. A revised$\backslash$nphylogenetic classification consistent With Oilanalytic results includes$\backslash$nthe following higher taxa: Glironiinae (for Glironia), Caluromyinae$\backslash$n(Caluromys and Caluromysiops), Hyladelphinae (Hyladelphys), Didelphinae$\backslash$n(Marmosini, Metachirini, Didelphini, and Thylamyini), Marmosini$\backslash$n(Marmosa, Monodelphis, and Tlacuatzin), Metachirini (Metachirus),$\backslash$nDidelphini (Chironectes, Didelphis, Lutreolina, and Philander) and$\backslash$nThylamyini (Chacodelphys, Cryptonanus, Gracilinanus, Lestodelphys,$\backslash$nMarmosops, and Thylamys). The probable relationships or several Neogene$\backslash$nfossil genera are also discussed. To facilitate identifications, all$\backslash$nRecent genera are redescribed, representative crania are illustrated,$\backslash$nand a key is provided.},
author = {Voss, R S and Jansa, S A},
doi = {10.1206/322.1},
issn = {0003-0090},
journal = {Bulletin of the American Museum of Natural History},
number = {322},
pages = {177},
title = {{Phylogenetic relationships and classification of Didelphid marsupials, an extant radiation of New World Metatherian Mammals}},
volume = {322},
year = {2009}
}
@article{Matthesius2011,
abstract = {The Janzen-Connell hypothesis proposes that density dependent seed and seedling mortality, combined with increasing seed and seedling survival away from the parent tree, together promote regular spacing of species and thus alpha diversity. This hypothesis has rarely been tested in tropical Africa, and rarely in montane forests anywhere. We tested this hypothesis using a combination of field experiments and observations in the most floristically diverse dry submontane forest in Nigeria. We investigated distance effects on seedling herbivory, seedling survival and seedling height growth. We found a significant decrease in herbivory with distance from conspecific adult trees for all three species of experimentally planted seedlings (Entandrophragma angolense, Deinbollia pinnata and Sterculia setigera), and also for naturally occurring seedlings of Pouteria altissima but not of Newtonia buchananii or Isolona pleurocarpa. The relative density of large seedlings/saplings of P. altissima, N. buchananii and I. pleurocarpa increased significantly at greater distance from conspecific adult trees; however, we found no significant distance effect on survival or height growth over 3 mo for all three experimentally planted species. Taken together, our results are some of the first to show that Janzen-Connell effects occur on the African continent and in particular montane tropical forest and suggest that such effects may be pantropical.},
author = {Matthesius, Arne and Chapman, Hazel and Kelly, Dave},
doi = {10.1111/j.1744-7429.2010.00664.x},
file = {::},
isbn = {0006-3606},
issn = {00063606},
journal = {Biotropica},
keywords = {Afromontane forest,Density dependence,Insect herbivory,Species diversity},
number = {1},
pages = {77--83},
title = {{Testing for Janzen-Connell effects in a west african montane forest}},
volume = {43},
year = {2011}
}
@article{Santilli2005,
abstract = {The current annual rates of tropical deforestation from Brazil and Indonesia alone would equal four-fifths of the emissions reductions gained by implementing the Kyoto Protocol in its first commitment period, jeopardizing the goal of Protocol to avoid dangerous anthropogenic interfer- ence with the climate system.We propose the novel concept of compensated reduction, whereby countries that elect to reduce national level deforestation to below a previously determined historical level would receive post facto compensation, and commit to stabilize or further reduce deforestation in the future. Such a program could create large-scale incentives to reduce tropical deforestation, as well as for broader developing country participation in the Kyoto Protocol, and leverage support for the continuity of the Protocol beyond the 20082012 first commitment period.},
author = {Santilli, M{\'{a}}rcio and Moutinho, Paulo and Schwartzman, Stephan and Nepstad, Daniel and Curran, Lisa and Nobre, Carlos},
doi = {10.1007/s10584-005-8074-6},
file = {::},
