title = {{Frugivoria e dispers{\~{a}}o de sementes de Rapanea lancifolia (Myrsinaceae) por aves numa {\'{a}}rea de cerrado do Estado de S{\~{a}}o Paulo, sudeste do Brasil}},
url = {http://web.me.com/galetti/Labic/Publications{\_}files/Francisco{\_}Galetti{\_}Ararajuba2001{\_}Rapanea.pdf},
