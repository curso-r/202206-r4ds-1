month = {apr},
number = {4},
pages = {281--289},
title = {{The spatial distribution of threats to species in Australia}},
url = {http://bioscience.oxfordjournals.org/cgi/doi/10.1525/bio.2011.61.4.8},
volume = {61},
year = {2011}
}
