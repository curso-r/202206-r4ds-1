journal = {Biota Neotropica},
keywords = {biotic complementarity,inventory,land-use planning,plot shape,plot size,survey,tropical forest},
number = {2},
pmid = {2595},
title = {{RAPELD: a modification of the Gentry method for biodiversity surveys in long-term ecological research sites}},
url = {http://www.scielo.br/scielo.php?script=sci{\_}arttext{\&}pid=S1676-06032005000300002{\&}lng=en{\&}nrm=iso{\&}tlng=en},
