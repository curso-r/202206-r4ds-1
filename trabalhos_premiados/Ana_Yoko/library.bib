keywords = {Arachnids,Resting metabolic rates,Sexual differences,Sexual selection},
month = {dec},
number = {4},
pages = {439--445},
pmid = {16314133},
title = {{Inter-sexual differences in resting metabolic rates in the Texas tarantula, {\textless}i{\textgreater}Aphonopelma anax{\textless}/i{\textgreater}}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S1095643305003296},
volume = {142},
