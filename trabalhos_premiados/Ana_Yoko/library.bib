journal = {Revista Brasileira de Ornitologia - Brazilian Journal of Ornithology},
number = {2},
pages = {90--298},
title = {{Annotated checklist of the birds of Brazil by the Brazilian Ornithological Records Committee / Lista comentada das aves do Brasil pelo Comitê Brasileiro de Registros Ornitológicos}},
volume = {23},
year = {2015}
}
