number = {5},
pages = {577--580},
title = {{Is more better? Sexual confusion during courtship between two sympatric and synchronic tarantulas: {\textless}i{\textgreater}Acanthoscurria suina{\textless}/i{\textgreater} and {\textless}i{\textgreater}Eupalaestrus weijenberghi{\textless}/i{\textgreater}}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84888412077{\&}partnerID=40{\&}md5=798224941c4df6f3a9b673340da1d65f},
