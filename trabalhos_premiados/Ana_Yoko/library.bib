title = {{Herpetofauna da floresta do baixo rio Moa em Cruzeiro do Sul, Acre - Brasil}},
url = {http://www.scielo.br/scielo.php?script=sci{\_}arttext{\&}pid=S1676-06032013000100023{\&}lng=pt{\&}tlng=pt},
