journal = {European Journal of Wildlife Research},
keywords = {Carnivores,Habitat fragmentation,Hierarchical partitioning,Montado,Roadkill,Stone marten},
number = {4},
pages = {707--716},
title = {{Do well-connected landscapes promote road-related mortality?}},
volume = {57},
year = {2011}
}
