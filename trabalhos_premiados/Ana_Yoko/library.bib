journal = {Phyllomedusa: Journal of Herpetology},
keywords = {Amazonian,Anura,Gymnophiona,Intraguild predation,Neotropical region,Predator-prey interaction},
month = {jun},
number = {1},
pages = {39},
title = {{Predation on amphibians by spiders (Arachnida, Araneae) in the Neotropical region}},
