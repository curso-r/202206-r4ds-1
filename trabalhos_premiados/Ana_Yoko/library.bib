issn = {1676-0603},
journal = {Biota Neotropica},
keywords = {additive parti-,atlantic forest,biotaneotropica,br,diversity patterns,habitat fragmentation,http,inter-annual variations,inventories,non-volant small mammals,org,pitfall traps,sampling methods,seasonal variations,spatial variations,tioning,www},
number = {2},
pages = {0--0},
title = {{Pequenos mam{\'{i}}feros n{\~{a}}o-voadores da Reserva Florestal do Morro Grande: distribui{\c{c}}{\~{a}}o das esp{\'{e}}cies e da diversidade em uma {\'{a}}rea de Mata Atl{\^{a}}ntica}},
