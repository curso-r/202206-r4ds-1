journal = {Animal Conservation},
keywords = {Amazon,Camera traps,Diversity,Inventory,Madre de Dios,Peru,Species accumulation curves,Species richness estimators},
number = {3},
pages = {169--178},
title = {{An evaluation of camera traps for inventorying large- and medium-sized terrestrial rainforest mammals}},
volume = {11},
year = {2008}
}
