number = {1/2},
pages = {3--225},
pmid = {25246403},
title = {{Mam{\'{i}}feros de m{\'{e}}dio e grande porte do Nordeste do Brasil: distribui{\c{c}}{\~{a}}o e taxonomia, com descri{\c{c}}{\~{a}}o de novas esp{\'{e}}cies}},
url = {http://periodicos.ufpb.br/ojs/index.php/revnebio/article/view/16716},
