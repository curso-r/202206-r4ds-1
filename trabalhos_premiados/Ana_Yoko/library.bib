@article{JacobsonSL2005,
abstract = {Highways cause significant impacts to birds in four$\backslash$n$\backslash$nways: direct mortality, indirect mortality, habitat fragmentation,$\backslash$n$\backslash$nand disturbance. In this paper I discuss$\backslash$n$\backslash$nhighway-related impacts, and suggest solutions from a$\backslash$n$\backslash$nhighway management perspective. Non-flying birds$\backslash$n$\backslash$n(either behaviorally or structurally) such as gallinaceous$\backslash$n$\backslash$nbirds and ducklings; waterbirds such as terns;$\backslash$n$\backslash$nowls; ground-nesters; scavengers; Neotropical overwater$\backslash$n$\backslash$nmigrants; frugivorous birds; and birds attracted$\backslash$n$\backslash$nto salt are often killed from highway-related causes.$\backslash$n$\backslash$nSuggested solutions include highway crossing structures,$\backslash$n$\backslash$ndiversion poles on bridges or medians, modified$\backslash$n$\backslash$nright-of-way mowing regimes, road kill removal,$\backslash$n$\backslash$nappropriate median vegetation, and modified deicing$\backslash$n$\backslash$nagents. Indirect mortalities caused by highway construction$\backslash$n$\backslash$nor maintenance include habitat loss and$\backslash$n$\backslash$ndecreased quality; predator attraction or bridges to$\backslash$n$\backslash$nnesting habitat; increased incidence of invasive species;$\backslash$n$\backslash$nincreased associated lethal structures; and maintenance$\backslash$n$\backslash$npractices that disrupt reproduction. Suggested$\backslash$n$\backslash$nsolutions include highway management strategies that$\backslash$n$\backslash$nconsider avian needs.},
