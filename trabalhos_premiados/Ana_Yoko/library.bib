pmid = {25246403},
title = {{OS ATAQUES REALIZADOS PELAS LONTRAS AOS TANQUES DE PEIXES E O CONHECIMENTO DOS PISCICULTORES PARA COM A LONTRA NEOTROPICAL Lontra longicaudis OLFERS, 1818 (CARNIVORA – MUSTELIDAE)}},
