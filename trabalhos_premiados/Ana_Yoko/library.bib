journal = {European Journal of Wildlife Research},
keywords = {Amazon region,Deforestation,Game harvest,Hunting techniques,Pioneer settlement,Subsistence hunting},
number = {3},
pages = {199--206},
title = {{The effects of deforestation on wildlife along the transamazon highway}},
volume = {51},
year = {2005}
}
