keywords = {amazon rainforest,drainage basin,environmental protection,forestry,geology,liana,structural basin},
number = {1},
pages = {103--105},
title = {{New records of Glironia venusta, Thomas, 1912 (Mammalia, Didelphidae), from the Amazon and ParaguaY Basins, Brazil}},
volume = {14},
year = {2007}
}
@article{Hansen2013,
abstract = {Quantification of global forest change has been lacking despite the recognized importance of forest ecosystem services. In this study, Earth observation satellite data were used to map global forest loss (2.3 million square kilometers) and gain (0.8 million square kilometers) from 2000 to 2012 at a spatial resolution of 30 meters. The tropics were the only climate domain to exhibit a trend, with forest loss increasing by 2101 square kilometers per year. Brazil's well-documented reduction in deforestation was offset by increasing forest loss in Indonesia, Malaysia, Paraguay, Bolivia, Zambia, Angola, and elsewhere. Intensive forestry practiced within subtropical forests resulted in the highest rates of forest change globally. Boreal forest loss due largely to fire and forestry was second to that in the tropics in absolute and proportional terms. These results depict a globally consistent and locally relevant record of forest change.},
author = {Hansen, M C},
doi = {10.1126/science.1244693},
file = {:Users/anameiga/OneDrive/Documents/Mendeley Desktop/Hansen{\_}2013{\_}High-Resolution Global Maps of.pdf:pdf},
number = {November},
pages = {123--134},
title = {{High-Resolution Global Maps of}},
volume = {850},
year = {2013}
}
@book{ICMBio2013b,
abstract = {A on{\c{c}}a-pintada {\'{e}} o maior carn{\'{i}}voro da Am{\'{e}}rica do Sul, o terceiro maior felino vivente do mundo e o {\'{u}}nico representante do g{\^{e}}nero Panthera (que inclui le{\~{o}}es, leopardos e tigres) no continente americano. Amplamente distribu{\'{i}}do por todo o Brasil, este mam{\'{i}}fero {\'{e}} considerado desde os tempos pr{\'{e}}-colombianos um s{\'{i}}mbolo de for{\c{c}}a e poder. As on{\c{c}}as-pintadas s{\~{a}}o provavelmente os animais mais populares do Brasil, estando presentes em cl{\'{a}}ssicos da literatura brasileira, letras de m{\'{u}}sicas e em manifesta{\c{c}}{\~{o}}es folcl{\'{o}}ricas. Infelizmente, as on{\c{c}}as t{\^{e}}m sido ca{\c{c}}adas desde o in{\'{i}}cio da coloniza{\c{c}}{\~{a}}o por raz{\~{o}}es que incluem aspectos: culturais, como a ca{\c{c}}a desportiva/recreativa; sociais, representando s{\'{i}}mbolo de status e for{\c{c}}a {\`{a}}quele que for capaz de capturar o animal; e econ{\^{o}}micos -num primeiro momento, o com{\'{e}}rcio das peles e mais recentemente em fun{\c{c}}{\~{a}}o de eventuais ataques a rebanhos bovinos e outros animais de cria{\c{c}}{\~{a}}o, gerando perdas financeiras aos criadores. Tal atividade resultou em representativas redu{\c{c}}{\~{o}}es populacionais e, embora caracterize um crime ambiental, ainda {\'{e}} largamente praticada em todo o pa{\'{i}}s. A perda e a extensa fragmenta{\c{c}}{\~{a}}o dos h{\'{a}}bitats, somada {\`{a}} ca{\c{c}}a t{\^{e}}m causado grandes preju{\'{i}}zos {\`{a}}s popula{\c{c}}{\~{o}}es de on{\c{c}}a-pintada em todos os biomas brasileiros onde a esp{\'{e}}cie ocorre, especialmente na Mata Atl{\^{a}}ntica e na Caatinga, onde as popula{\c{c}}{\~{o}}es sofreram dr{\'{a}}sticas redu{\c{c}}{\~{o}}es. Uma das atribui{\c{c}}{\~{o}}es do Instituto Chico Mendes de Conserva{\c{c}}{\~{a}}o da Biodiversidade {\'{e}} o desenvolvimento e a aplica{\c{c}}{\~{a}}o de estrat{\'{e}}gias voltadas {\`{a}} conserva{\c{c}}{\~{a}}o da biodiversidade brasileira, conforme estabelecido na Portaria Conjunta MMA/ICMBio n o 316 de agosto de 2009. Dessa forma, considerando a import{\^{a}}ncia biol{\'{o}}gica e ecol{\'{o}}gica de um dos maiores predadores brasileiros, foi pactuado com diversos representantes institucionais um conjunto ordenado de a{\c{c}}{\~{o}}es na forma do Plano de A{\c{c}}{\~{a}}o Nacional para a Conserva{\c{c}}{\~{a}}o da On{\c{c}}a-pintada -PAN On{\c{c}}a-pintada. Apoio Parceiros Realiza{\c{c}}{\~{a}}o Minist{\'{e}}rio do Meio Ambiente PANTHERA LEADERS IN WILD CAT CONSERVATION},
