@article{Martins2005a,
abstract = {S{\~{a}}o introduzidos os principais {\'{i}}ndices de diversidade, dentre os mais usados na literatura especializada. Os {\'{i}}ndices s{\~{a}}o classificados como medidas de riqueza (tipo I), medidas de abund{\^{a}}ncia (tipo II) e medidas de heterogeneidade (tipo III). O significado de cada {\'{i}}ndice, suas vantagens e desvantagens e as situa{\c{c}}{\~{o}}es em que seu uso {\'{e}} mais conveniente s{\~{a}}o discutidos. A influ{\^{e}}ncia da t{\'{e}}cnica de amostragem, especialmente o tamanho m{\'{i}}nimo amostr{\'{a}}vel, sobre os resultados dos {\'{i}}ndices tamb{\'{e}}m {\'{e}} abordada. Algumas das principais quest{\~{o}}es sobre a estimativa da biodiversidade s{\~{a}}o discutidas: quais dados devem ser levantados? {\'{E}} mais conveniente usar {\'{a}}rea ou n{\'{u}}mero de indiv{\'{i}}duos amostrados? Quais {\'{i}}ndices devem ser usados? Como fazer compara{\c{c}}{\~{o}}es? Ressalta-se que uma medida de diversidade {\'{e}} um par{\^{a}}metro extremamente reducionista, que objetiva expressar toda a complexidade estrutural, a riqueza espec{\'{i}}fica e as intera{\c{c}}{\~{o}}es bi{\'{o}}ticas e abi{\'{o}}ticas de uma comunidade ecol{\'{o}}gica atrav{\'{e}}s de um {\'{u}}nico n{\'{u}}mero. Al{\'{e}}m disso, embora alguns padr{\~{o}}es mundiais sejam conhecidos (como, por exemplo, a rela{\c{c}}{\~{a}}o direta entre diversidade e produtividade prim{\'{a}}ria e a rela{\c{c}}{\~{a}}o inversa entre diversidade e latitude), geralmente a diversidade {\'{e}} tomada em rela{\c{c}}{\~{a}}o a um t{\'{a}}xon de uma certa categoria, admitindo-se a premissa de que a diversidade desse t{\'{a}}xon represente a diversidade do sistema analisado. Al{\'{e}}m disso, ainda n{\~{a}}o h{\'{a}} uma maneira bem estabelecida de expressar a biodiversidade de uma comunidade como um todo.},
author = {Martins, F R and Santos, F a M},
file = {::},
issn = {18063756},
journal = {Holos},
number = {6},
pages = {236--267},
pmid = {19618038},
title = {{T{\'{e}}cnicas usuais de estimativa da biodiversidade}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19618038},
volume = {1},
year = {2005}
}
