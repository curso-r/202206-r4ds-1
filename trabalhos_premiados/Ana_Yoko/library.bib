journal = {Natureza {\&} Conserva{\c{c}}{\~{a}}o},
keywords = {a trabalhos com amplo,artigos,compilados pelos sistemas scopus,dando assim prefer{\^{e}}ncia a,http,internacionais e,limitei tamb{\'{e}}m a busca,ou,publicados em revistas cient{\'{i}}ficas,respaldo internacional,scopus,www},
